@Master['default.sshtml']
@Section['content']

  <header>
  <div class="head-inner">
    <h1><a href="/category/">Categories</a> - ASP.NET MVC</h1>
  </div>
  </header>

  <ul class="category-list fancy-container orange">
    @Each.CategoriesInPost
      <li>
        <a href="@Current.Url" title="@Current.Title">@Current.Title</a> 
        <small>(@Current.DateFormatted)</small>
      </li>
    @EndEach
  </ul>
  
@EndSection