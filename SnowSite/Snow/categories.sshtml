@Master['default.sshtml']
@Section['content']

  <header>
  <div class="head-inner">
    <h1>Categories</h1>
  </div>
  </header>
  
  <ul class="category-list fancy-container darkishblue">
    @Each.Categories
      <li><a href="/category/@Current.Url" title="@Current.Name">@Current.Name</a></li>
    @EndEach
  </ul>
  
@EndSection