@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Sandra.Snow.PreCompiler.ViewModels.ContentViewModel>
@using System.Collections.Generic
@{
  Layout = "default.cshtml";
}

  <header>
  <div class="head-inner">
    <h1>Categories</h1>
  </div>
  </header>
  
  <ul class="category-list fancy-container darkishblue">
    @foreach(var category in Model.Categories){
      <li><a href="/category/@category.Url" title="@category.Name">@category.Name</a></li>
    }
  </ul>
  
